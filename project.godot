"class": "Downloader",
"language": "GDScript",
"path": "res://src/versions/Downloader.gd"
}, {
"base": "Reference",
"class": "ProjectCreator",
"language": "GDScript",
"path": "res://src/dialogs/ProjectCreator.gd"
}, {
"base": "HBoxContainer",
"class": "ProjectListItem",
"language": "GDScript",
"path": "res://src/projects/ProjectListItem.gd"
}, {
"base": "Object",
"class": "Utils",
"language": "GDScript",
"path": "res://src/Utils.gd"
}, {
"base": "OptionButton",
"class": "VersionDropdown",
"language": "GDScript",
"path": "res://src/misc/VersionDropdown.gd"
}, {
"base": "HTTPRequest",
"class": "VersionsUpdater",
"language": "GDScript",
"path": "res://src/versions/VersionsUpdater.gd"
} ]
_global_script_class_icons={
"Downloader": "",
"ProjectCreator": "",
